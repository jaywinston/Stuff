class Error {

    static int _errno;
    static int TABSIZE;
    static Array ERRTAB;

    function void init(Array tab, int ts) {
        let ERRTAB = tab;
        let TABSIZE = ts;
        let _errno = 0;
        return;
    }

    function void dispose() {
        var int i;
        var String s;

        let i = 0;
        while (i < TABSIZE) {
            let s = ERRTAB[i];
            do s.dispose();
            let i = i + 1;
        }

        do ERRTAB.dispose();

        return;
    }

    function void set(int i) {
        let _errno = i;
        return;
    }

    function int n() {
        return _errno;
    }

    function String msg() {
        return ERRTAB[_errno];
    }

    function String clear() {
        let _errno = 0;
        return lib.NULLS();
    }

}
