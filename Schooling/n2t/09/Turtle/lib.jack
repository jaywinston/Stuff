class lib {

    static int maxlen;
    static String nulls;

    function void init() {

        let maxlen = 50;
        let nulls = "";
        return;
    }

    function int mod(int n, int d) {
        var int q;

        let q = n / d;
        return n - (q*d);
    }

    function int MAXLEN() {
        return maxlen;
    }

    function String NULLS() {
        return nulls;
    }

}
