class Vector {

    field int x, y;

    constructor Vector new() {
        return this;
    }

    method Vector calculate(int m, int a) {
        let x = (m*lib.cos100(a)) / 100;
        let y = (m*lib.sin100(a)) / 100;
        return this;
    }

    method void dispose() {
        do Memory.deAlloc(this);
        return;
    }

    method int x() {
        return x;
    }

    method int y() {
        return y;
    }
}
