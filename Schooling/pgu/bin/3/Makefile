	as -o ../../lib/3/maximum.o ../../src/3/maximum.s
	ld -o maximum ../../lib/3/maximum.o
maximum-2: ../../src/3/maximum-2.s
	as -o ../../lib/3/maximum-2.o ../../src/3/maximum-2.s
	ld -o maximum-2 ../../lib/3/maximum-2.o
maximum-3: ../../src/3/maximum-3.s
	as -o ../../lib/3/maximum-3.o ../../src/3/maximum-3.s
	ld -o maximum-3 ../../lib/3/maximum-3.o
maximum-4: ../../src/3/maximum-4.s
	as -o ../../lib/3/maximum-4.o ../../src/3/maximum-4.s
	ld -o maximum-4 ../../lib/3/maximum-4.o
minimum: ../../src/3/minimum.s
	as -o ../../lib/3/minimum.o ../../src/3/minimum.s
	ld -o minimum ../../lib/3/minimum.o

